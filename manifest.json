{
  "manifest_version": 2,

  "name": "Mopsus",
  "description": "This should save your webpage. Should.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "content": {
    "scripts": ["content.js"]
  },

  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ]
}
